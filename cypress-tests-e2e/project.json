{
  "name": "cypress-tests-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "cypress-tests-e2e/src",
  "tags": [],
  "implicitDependencies": ["cypress-tests"],
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "cypress-tests-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "cypress-tests:serve:development"
      },
      "configurations": {
        "production": {
          "devServerTarget": "cypress-tests:serve:production"
        },
        "ci": {
          "devServerTarget": "cypress-tests:serve-static"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
